{
  "name": "louie-web",
  "version": "1.0.1",
  "description": "Web interface for Louie",
  "license": "MIT",
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "contributors": [
    {
      "name": "Jørgen Johansen",
      "email": "post@jorgenjohansen.no",
      "url": "https://jorgenjohansen.no"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.2.4"
  },
  "scripts": {
    "start": "node standalone.js",
    "test": "standard && tap --reporter=spec test/*.js",
    "coverage": "tap test/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install && node setup.js",
    "generate-test-token": "node utils/generate-test-token.js"
  },
  "keywords": [
    "Louie"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "The Kingsmen",
    "versionVideoUrl": "https://www.youtube.com/watch?v=4V1p1dM3snQ",
    "dusteNavn": "MinElev"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/telemark/louie-web.git"
  },
  "bugs": {
    "url": "http://github.com/telemark/louie-web/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "standard": "^8.6.0",
    "tap": "^10.3.0"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "hapi": "^16.1.0",
    "hapi-auth-cookie": "^6.1.1",
    "hoek": "^4.1.0",
    "inert": "^4.1.0",
    "jsonwebtoken": "^7.2.1",
    "ldapauth-fork": "^3.0.0",
    "mongojs": "^2.3.0",
    "path": "^0.12.7",
    "vision": "^4.0.1",
    "wreck": "^10.0.0",
    "yar": "^8.1.2"
  }
}
